{
  "name": "basic-accounting",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "config": {
    "jsx": "src/jsx/navigation.jsx src/jsx/help.jsx src/jsx/journal.jsx src/jsx/indexList.jsx src/jsx/mobileBalanceSheetTable.jsx src/jsx/admin.jsx src/jsx/home.jsx",
    "css": "src/css/main.css src/css/mobile.css src/css/index.css"
  },
  "directories.man": {
    "js": "./src/js",
    "jsx:": "./src/jsx",
    "css": "./src/css",
    "html": "./src/templates"
  },
  "scripts": {
    "serve": "python app.py",
    "css": "esbuild %npm_package_config_css% --bundle --outdir=dist/styles/ --out-extension:.css=.min.css --minify --tree-shaking=true",
    "css:watch": "esbuild %npm_package_config_css% --bundle --outdir=dist/styles/ --entry-names=[name].min --minify --tree-shaking=true --watch",
    "build": "npm run jsx & npm run css & node bin/es.js",
    "dev": "npm-run-all --parallel jsx:watch css:watch js:watch serve",
    "js": "node bin/es.js",
    "js:watch": "node bin/es.js --watch",
    "jsx": "esbuild %npm_package_config_jsx% --bundle --outdir=dist/react/compiled/ --loader:.js=jsx --drop:debugger --tree-shaking=true --minify",
    "jsx:watch": "esbuild %npm_package_config_jsx% --bundle --outdir=dist/react/compiled/ --loader:.js=jsx --drop:debugger --tree-shaking=true --minify --watch"
  },
  "keywords": [],
  "author": "5idneyD",
  "license": "",
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@chakra-ui/react": "^2.5.3",
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@mui/icons-material": "^5.11.16",
    "babel-loader": "^9.1.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "esbuild": "0.17.12",
    "framer-motion": "^10.9.1",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve-glob": "^1.0.0",
    "style-loader": "^3.3.2",
    "webpack": "^5.76.2"
  },
  "devDependencies": {
    "esbuild-plugin-import-glob": "^0.1.1"
  }
}
